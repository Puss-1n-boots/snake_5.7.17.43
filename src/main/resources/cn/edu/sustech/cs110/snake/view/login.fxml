<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cn.edu.sustech.cs110.snake.control.LoginController">
    <VBox alignment="CENTER" prefHeight="179.0" prefWidth="282.0" spacing="20.0">
        <Label text="用户名" />
        <TextField fx:id="usernameField" prefHeight="23.0" prefWidth="287.0" />
        <Label text="密码" />
        <PasswordField fx:id="passwordField" />
        <Button fx:id="LoginInButton" onAction="#handleLoginButtonAction" text="登录" />
    </VBox>
</AnchorPane>
