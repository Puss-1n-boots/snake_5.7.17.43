<AnchorPane prefHeight="400.0" prefWidth="640.0" style="-fx-background-color: #292929;" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cn.edu.sustech.cs110.snake.control.StartController">
    <children>
        <ImageView fitHeight="422.0" fitWidth="653.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@background.jpg" />
            </image>
        </ImageView>
        <StackPane layoutX="203.0" layoutY="83.0" />
        <HBox layoutX="310.0" layoutY="300.0" spacing="50.0" />
         <Button fx:id="playButton" layoutX="270.0" layoutY="211.0" mnemonicParsing="false" onAction="#startPlay" style="-fx-background-color: #ff6d00;" text="Play">
             <font>
                 <Font name="Futura Hv BT" size="24.0" />
             </font>
         </Button>
         <Button fx:id="loginButton" layoutX="203.0" layoutY="313.0" mnemonicParsing="false" style="-fx-background-color: #ff6d00;" text="登录">
             <font>
                 <Font name="Futura Hv BT" size="24.0" />
             </font>
         </Button>
      <Button fx:id="set" layoutX="331.0" layoutY="313.0" mnemonicParsing="false" style="-fx-background-color: #ff6d00;" text="设置">
         <font>
            <Font name="Futura Hv BT" size="24.0" />
         </font>
      </Button>
    </children>
</AnchorPane>
